<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Acme\TestBundle\Entity\PlayerDeaths" table="player_deaths">
    <indexes>
      <index name="date" columns="date"/>
      <index name="player_id" columns="player_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="bigint" column="date" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="level" type="integer" column="level" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <many-to-one field="player" target-entity="Players" fetch="LAZY">
      <join-columns>
        <join-column name="player_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
