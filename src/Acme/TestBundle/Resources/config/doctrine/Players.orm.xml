<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Acme\TestBundle\Entity\Players" table="players">
    <indexes>
      <index name="account_id" columns="account_id"/>
      <index name="group_id" columns="group_id"/>
      <index name="online" columns="online"/>
      <index name="deleted" columns="deleted"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="name" columns="name,deleted"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="worldId" type="boolean" column="world_id" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="groupId" type="integer" column="group_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="level" type="integer" column="level" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="vocation" type="integer" column="vocation" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="health" type="integer" column="health" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">150</option>
      </options>
    </field>
    <field name="healthmax" type="integer" column="healthmax" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">150</option>
      </options>
    </field>
    <field name="experience" type="bigint" column="experience" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lookbody" type="integer" column="lookbody" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lookfeet" type="integer" column="lookfeet" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lookhead" type="integer" column="lookhead" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="looklegs" type="integer" column="looklegs" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="looktype" type="integer" column="looktype" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">136</option>
      </options>
    </field>
    <field name="lookaddons" type="integer" column="lookaddons" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="maglevel" type="integer" column="maglevel" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mana" type="integer" column="mana" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="manamax" type="integer" column="manamax" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="manaspent" type="integer" column="manaspent" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="soul" type="integer" column="soul" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="townId" type="integer" column="town_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="posx" type="integer" column="posx" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="posy" type="integer" column="posy" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="posz" type="integer" column="posz" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="conditions" type="blob" column="conditions" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cap" type="integer" column="cap" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sex" type="integer" column="sex" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastlogin" type="bigint" column="lastlogin" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastip" type="integer" column="lastip" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="save" type="boolean" column="save" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="skull" type="boolean" column="skull" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="skulltime" type="integer" column="skulltime" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="rankId" type="integer" column="rank_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="guildnick" type="string" column="guildnick" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="lastlogout" type="bigint" column="lastlogout" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="blessings" type="boolean" column="blessings" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="balance" type="bigint" column="balance" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="stamina" type="bigint" column="stamina" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">stored in miliseconds</option>
        <option name="default">151200000</option>
      </options>
    </field>
    <field name="direction" type="integer" column="direction" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">2</option>
      </options>
    </field>
    <field name="lossExperience" type="integer" column="loss_experience" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">100</option>
      </options>
    </field>
    <field name="lossMana" type="integer" column="loss_mana" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">100</option>
      </options>
    </field>
    <field name="lossSkills" type="integer" column="loss_skills" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">100</option>
      </options>
    </field>
    <field name="lossContainers" type="integer" column="loss_containers" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">100</option>
      </options>
    </field>
    <field name="lossItems" type="integer" column="loss_items" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">100</option>
      </options>
    </field>
    <field name="premend" type="integer" column="premend" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">NOT IN USE BY THE SERVER</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="online" type="boolean" column="online" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="marriage" type="integer" column="marriage" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="promotion" type="integer" column="promotion" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="deleted" type="boolean" column="deleted" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <many-to-one field="account" target-entity="Accounts" fetch="LAZY">
      <join-columns>
        <join-column name="account_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
