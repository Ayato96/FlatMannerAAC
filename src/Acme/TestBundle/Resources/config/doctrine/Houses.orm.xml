<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Acme\TestBundle\Entity\Houses" table="houses">
    <unique-constraints>
      <unique-constraint name="id" columns="id,world_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="worldId" type="boolean" column="world_id" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="owner" type="integer" column="owner" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="paid" type="integer" column="paid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="warnings" type="integer" column="warnings" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastwarning" type="integer" column="lastwarning" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="town" type="integer" column="town" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="size" type="integer" column="size" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="price" type="integer" column="price" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="rent" type="integer" column="rent" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="doors" type="integer" column="doors" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="beds" type="integer" column="beds" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tiles" type="integer" column="tiles" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="guild" type="boolean" column="guild" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="clear" type="boolean" column="clear" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
