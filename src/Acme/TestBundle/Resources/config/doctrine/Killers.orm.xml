<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Acme\TestBundle\Entity\Killers" table="killers">
    <indexes>
      <index name="death_id" columns="death_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="finalHit" type="boolean" column="final_hit" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="unjustified" type="boolean" column="unjustified" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="death" target-entity="PlayerDeaths" fetch="LAZY">
      <join-columns>
        <join-column name="death_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
