<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Acme\TestBundle\Entity\Guilds" table="guilds">
    <unique-constraints>
      <unique-constraint name="name" columns="name,world_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="worldId" type="boolean" column="world_id" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ownerid" type="integer" column="ownerid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="creationdata" type="integer" column="creationdata" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="motd" type="string" column="motd" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
